class Solution {
    public boolean checkRecord(String s) {
        int n = s.length(),A=0,L=0,count,t;
        for(int i=0; i<n; i++){
            if(s.charAt(i) == 'A'){
                A++;
            }
            if(A>=2){
                return false;
            }
        }
        if(s.contains("LLL")){
            return false;
        }
        return true;
        
    }
}
